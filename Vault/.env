#Postgress
POSTGRES_USER=vault
POSTGRES_PASSWORD=strongpassword
POSTGRES_DB=vault
POSTGRES_HOST=postgresHost
POSTGRES_PORT=5432
POSTGRES_LOG_LEVEL=4

# Vault
VAULT_ADDR=http://vault:8200
VAULT_TOKEN=root
# Vault Configuration
VAULT_DEV_ROOT_TOKEN_ID=root
# Vault Storage Backend
VAULT_STORAGE_BACKEND=postgres
# Vault PostgreSQL Connection
VAULT_STORAGE_POSTGRES_HOST=postgres
VAULT_STORAGE_POSTGRES_PORT=5432
VAULT_STORAGE_POSTGRES_USER=vault
VAULT_STORAGE_POSTGRES_PASSWORD=strongpassword
# Vault PostgreSQL Database
VAULT_STORAGE_POSTGRES_DB=vault
# Vault Listener
VAULT_LISTENER_TCP_ADDRESS=0.0.0.0:8200
VAULT_ADDR=http://0.0.0.0:8200      
# Vault API
VAULT_API_ADDR=http://              
# Vault UI      
VAULT_UI_ADDR=http://   
# Vault Audit
VAULT_AUDIT_FILE_PATH=/var/log/vault_audit.log
# Vault TLS Configuration   
VAULT_TLS_CERT_FILE=/etc/vault/tls  
VAULT_TLS_KEY_FILE=/etc/vault/tls/key.pem
# Vault Telemetry
VAULT_TELEMETRY_ENABLED=true
# Vault Performance Standby
VAULT_PERFORMANCE_STANDBY_ENABLED=false
# Vault Seal Configuration
VAULT_SEAL_TYPE=shamir
# Vault Shamir Seal Configuration
VAULT_SHAMIR_SECRET_SHARES=5
VAULT_SHAMIR_SECRET_THRESHOLD=3
# Vault Enterprise Features
VAULT_ENTERPRISE_ENABLED=false
# Vault Plugin Directory
VAULT_PLUGIN_DIR=/vault/plugins
# Vault Plugin Configuration
VAULT_PLUGIN_CONFIG=/vault/plugin-config
# Vault Plugin Registration
VAULT_PLUGIN_REGISTRATION_ENABLED=true
# Vault Plugin Registration Address
VAULT_PLUGIN_REGISTRATION_ADDRESS=http://vault:8200
# Vault Plugin Registration Token
VAULT_PLUGIN_REGISTRATION_TOKEN=root
# Vault Plugin Registration Interval
VAULT_PLUGIN_REGISTRATION_INTERVAL=60s
# Vault Plugin Registration Timeout
VAULT_PLUGIN_REGISTRATION_TIMEOUT=30s
# Vault Plugin Registration Max Retries
VAULT_PLUGIN_REGISTRATION_MAX_RETRIES=3
# Vault Plugin Registration Retry Interval
VAULT_PLUGIN_REGISTRATION_RETRY_INTERVAL=10s
# Vault Plugin Registration Health Check
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_ENABLED=true
# Vault Plugin Registration Health Check Interval    
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_INTERVAL=30s
# Vault Plugin Registration Health Check Timeout
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_TIMEOUT=10s
# Vault Plugin Registration Health Check Max Retries
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_MAX_RETRIES=3
# Vault Plugin Registration Health Check Retry Interval
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_RETRY_INTERVAL=10s
# Vault Plugin Registration Health Check URL
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_URL=http://vault:8200/v1/sys/health
# Vault Plugin Registration Health Check Method
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_METHOD=GET
# Vault Plugin Registration Health Check Headers
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_HEADERS=Content-Type:application/json
# Vault Plugin Registration Health Check Body
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_BODY={}
# Vault Plugin Registration Health Check Expected Status Codes
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_STATUS_CODES=200,429
# Vault Plugin Registration Health Check Expected Response
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_RESPONSE={"status":"success"}  
# Vault Plugin Registration Health Check Expected Response Headers
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_RESPONSE_HEADERS=Content-Type:application/json
# Vault Plugin Registration Health Check Expected Response Body
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_RESPONSE_BODY={}
# Vault Plugin Registration Health Check Expected Response Status Code
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_RESPONSE_STATUS_CODE=200
# Vault Plugin Registration Health Check Expected Response Headers
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_RESPONSE_HEADERS=Content-Type:application/json
# Vault Plugin Registration Health Check Expected Response Body
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_RESPONSE_BODY={}
# Vault Plugin Registration Health Check Expected Response Status Code
VAULT_PLUGIN_REGISTRATION_HEALTH_CHECK_EXPECTED_RESPONSE_STATUS_CODE=200